<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?eclipse version="3.0"?>

<plugin>
  <extension id="OntovizViewComponent"
              point="org.protege.editor.core.application.ViewComponent">
      <label value="OntoViz View"/>
      <class value="ontoviz.OntovizImportView"/>
      <headerColor value="@org.protege.classcolor"/>
      <category value="@org.protege.ontologycategory"/>
   </extension>

   <extension id="OntoVizTab"
              point="org.protege.editor.core.application.WorkspaceTab">
      <label value="OntoViz"/>
      <class value="org.protege.editor.owl.ui.OWLWorkspaceViewsTab"/>
      <index value="X"/>
      <editorKitId value="OWLEditorKit"/>
      <defaultViewConfigFileName value="viewconfig-ontoviz.xml"/>
   </extension>
</plugin>
